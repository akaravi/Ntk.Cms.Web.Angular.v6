  "version": "15.1.11",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve1": "node --max_old_space_size=4048 ./node_modules/@angular/cli/bin/ng serve",
    "serve2": "node --max_old_space_size=24000 ./node_modules/@angular/cli/bin/ng serve",
    "build-prod": "node --max_old_space_size=24000 ./node_modules/@angular/cli/bin/ng build --configuration production --build-optimizer",
    "build-prod2": "node --max_old_space_size=24000 ./node_modules/@angular/cli/bin/ng build --prod --build-optimizer",
    "build-prod3": "ng build --prod",
    "analyze": "ng build --prod --stats-json && webpack-bundle-analyzer dist/ntk-cms-web/stats.json",
    "start-pwa": "ng build --prod && http-server -p 8080 -c-1 dist/ntk-cms-web",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
